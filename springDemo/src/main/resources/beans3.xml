<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"><!--default-init-method="init" default-destroy-method="destroy"默认销毁-->

    <!-- Definition for student bean-->
    <bean id="student" class="com.test1.Student">
        <property name="name" value="Zara"/>
        <!-- try without passing age and check the result -->
        <property name="age" value="11"/>
    </bean>
    <bean id="profile" class="com.test1.Profile">
    </bean>
    <bean id="student1" class="com.test1.Student">
        <property name="name" value="Zara"/>
        <property name="age" value="11"/>
    </bean>
    <bean id="student2" class="com.test1.Student">
        <property name="name" value="Nuha"/>
        <property name="age" value="2"/>
    </bean>
</beans>


